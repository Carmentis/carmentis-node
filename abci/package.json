{
  "name": "abci",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "engines": {
    "node": ">=22.18.0"
  },
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:profiling": "node --prof dist/src/main.js",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "gen:proto": "node scripts/gen-proto.js"
  },
  "dependencies": {
    "@bufbuild/protobuf": "^2.2.5",
    "@cmts-dev/carmentis-sdk": "^1.13.1",
    "@grpc/grpc-js": "^1.13.3",
    "@grpc/proto-loader": "^0.7.15",
    "@grpc/reflection": "^1.0.4",
    "@iarna/toml": "^2.2.5",
    "@logtape/file": "^1.2.2",
    "@logtape/logtape": "^1.2.0",
    "@logtape/otel": "^1.2.2",
    "@nestjs/axios": "^4.0.0",
    "@nestjs/common": "^11",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11",
    "@nestjs/microservices": "^11",
    "@nestjs/platform-express": "^11",
    "@nestjs/platform-socket.io": "^11",
    "@nestjs/platform-ws": "^11",
    "@nestjs/websockets": "^11",
    "@noble/secp256k1": "^2.3.0",
    "cbor-x": "^1.6.0",
    "get-port": "^7.1.0",
    "level": "^10.0.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2",
    "ts-proto": "^2.7.0",
    "valibot": "^1.2.0",
    "ws": "^8.18.1",
    "zod": "^4.1.3"
  },
  "devDependencies": {
    "@bufbuild/buf": "^1.53.0",
    "@nestjs/cli": "^11",
    "@nestjs/schematics": "^11",
    "@nestjs/testing": "^11",
    "@protobuf-ts/plugin": "^2.9.6",
    "@types/express": "^5.0.1",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.17.31",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^8.31.0",
    "@typescript-eslint/parser": "^8.31.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.6",
    "jest": "^29.7.0",
    "prettier": "^3.5.3",
    "protoc": "^1.1.3",
    "protoc-bin": "^3.10.0",
    "source-map-support": "^0.5.21",
    "supertest": "^7.1.0",
    "ts-jest": "^29.3.2",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "ts-protoc-gen": "^0.15.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.8.3"
  },
  "packageManager": "pnpm@10.12.4",
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
